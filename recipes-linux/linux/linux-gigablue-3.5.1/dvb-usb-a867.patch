+	.suspend    = af903x_suspend,
+	.resume     = af903x_resume,
+#if LINUX_VERSION_CODE >  KERNEL_VERSION(2,6,22)
+	.reset_resume = af903x_resume,
+#endif
+	.supports_autosuspend = 1,
+};
