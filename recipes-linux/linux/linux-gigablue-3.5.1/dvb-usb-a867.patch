+static int __init af903x_module_init(void)
+{
+	int result;
+
+	printk("AVerMedia A867 driver module V%s loaded.\n", DRIVER_VER);
+
+	if ((result = usb_register(&af903x_driver))) {
+		err("usb_register failed. Error number %d",result);
+		return result;
+	}
+	return 0;
