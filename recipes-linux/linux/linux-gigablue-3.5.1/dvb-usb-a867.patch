+    error = Standard_readRegisterBits (demodulator, chip, Processor_OFDM, r_mp2if_sync_byte_locked, mp2if_sync_byte_locked_pos, mp2if_sync_byte_locked_len, &temp);
+    if (error) goto exit;
+    if (temp) *locked = True;
+
+exit :
+#endif
+
+    if(error) {
+	deb_info("%s error, ret=0x%x\n", __FUNCTION__, error);
+    }
+    return (error);
+}
