+			dwError = Demodulator_writeRegister((Demodulator*) &pdc->Demodulator, 0, Processor_OFDM, p_mp2if_psb_overflow, ucValue&(~0x01));
+			if( dwError ) goto exit;
+		}
+	}
+
+	if( bPsbOverflow ) *bPsbOverflow = PsbOverflow;
+exit:
