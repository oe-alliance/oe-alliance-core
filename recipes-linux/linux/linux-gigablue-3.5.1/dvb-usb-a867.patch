+	af903x_stop_monitor_thread(demod);
+
+        error = DL_ApCtrl(0);
+        if (error) {
+		deb_data("%s calling DL_ApCtrl error : 0x%x\n", __FUNCTION__, error);
+		return -EIO;
+	}
+
+	usb_autopm_put_interface(uintfs);	
+
+	deb_data("- Exit %s Function -\n",__FUNCTION__);
+	return 0;
