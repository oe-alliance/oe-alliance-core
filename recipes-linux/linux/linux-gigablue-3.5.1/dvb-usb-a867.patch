+#ifndef wait_event_timeout
+#define wait_event_timeout(wq, condition, timeout)                   	     \
+({                                                                           \
+     long __ret = timeout;                                                   \
+     if (!(condition))                                                       \
+     do {                                                                    \
+	     DEFINE_WAIT(__wait);                                            \
+	     for (;;) {                                                      \
+		     prepare_to_wait(&wq, &__wait, TASK_UNINTERRUPTIBLE);    \
+		     if (condition)                                          \
+			 break;                                              \
+		     __ret = schedule_timeout(__ret);                        \
+		     if (!__ret)                                             \
+			 break;                                              \
+	     }                                                               \
+	     finish_wait(&wq, &__wait);                                      \
+     } while (0);							     \
+     __ret;                                                                  \
+})
+#endif
