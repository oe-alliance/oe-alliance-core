+    if (temp)
+        channelModulation->priority = Priority_HIGH;
+    else
+        channelModulation->priority = Priority_LOW;
+
+    /** Get high code rate */
+    error = Standard_readRegisterBits (demodulator, chip, Processor_OFDM, g_reg_tpsd_hpcr, reg_tpsd_hpcr_pos, reg_tpsd_hpcr_len, &temp);
