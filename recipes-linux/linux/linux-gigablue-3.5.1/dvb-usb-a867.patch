+Dword Standard_divider (
+    IN  Demodulator*    demodulator,
+    IN  Dword           a,
+    IN  Dword           b,
+    IN  Dword           x
+) {
+    Dword answer = 0;
+    Dword c = 0;
+    Dword i = 0;
+
+    if (a > b) {
+        c = a / b;
+        a = a - c * b;
+    }
+
+    for (i = 0; i < x; i++) {
+        if (a >= b) {
+            answer += 1;
+            a-=b;
+        }
+        a <<= 1;
+        answer <<= 1;
+    }
+
+    answer = (c << (Long) x) + answer;
+
+    return (answer);
+}
