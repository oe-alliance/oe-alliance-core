+// return 1 if the difference between freq1 & freq2 is smaller or equal than t.
+static inline int Is_Within_Tolerance(u32 freq1, u32 freq2, u32 t)
+{
+	u32 diff = (freq1>freq2)? (freq1-freq2) : (freq2-freq1);
+	if( diff<=t ) return 1;
+	else return 0;
+}
