+	.release       = tuner_release,
+
+	.init          = tuner_init,
+	.sleep         = tuner_sleep,
+
+	.set_params    = tuner_set_params,
+	.get_frequency = tuner_get_frequency,
+	.get_bandwidth = tuner_get_bandwidth
+};
