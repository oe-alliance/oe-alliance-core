+
+#define PNX8XXX_CLK_USB_30_CTL_0  (PNX8XXX_CLOCK_BASE+0x264)
+#define PNX8XXX_CLK_USB_30_CTL_1  (PNX8XXX_CLOCK_BASE+0x444)
+#define PNX8XXX_CLK_USB_30_CTL_2  (PNX8XXX_CLOCK_BASE+0x270)
