+ 
+static struct resource apollo_gcs_resources[] = {
+    {
+        .start      = APOLLO_GCS_CFG_BASE,
+        .end        = (APOLLO_GCS_CFG_BASE + APOLLO_GCS_CFG_LEN - 1),
