+#define PNX8XXX_DFT_FREQ_CTL    (PNX8XXX_CLOCK_BASE+0xC00)
+#define PNX8XXX_DFT_DONE_MASK   0x00000002
+#define PNX8XXX_DFT_RESULT_MASK 0xFFFF0000
