+#define USB_AHB_SRC_PLL		0x01
+
+#define ENABLE_CLOCK(clockreg)	SET_CLOCK(clockreg,1)
+#define DISABLE_CLOCK(clockreg)	SET_CLOCK(clockreg,0)
