+		.coherent_dma_mask = 0xFFFFFFFF,
+	},
+};
+
+static int __init apollo_sfc_init(void)
+{
+   return platform_device_register (&apollo_sfc_device);
+}
+
+arch_initcall(apollo_sfc_init);
