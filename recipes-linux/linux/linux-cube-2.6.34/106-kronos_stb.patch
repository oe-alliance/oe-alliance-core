+u32 __initdata uARM926_start, uARM926_size=0;
+u32 __initdata uBIS_start, uBIS_size=0;
+u32 __initdata uADSP_start, uADSP_size=0;
+u32 __initdata uVDSP_start, uVDSP_size=0;
