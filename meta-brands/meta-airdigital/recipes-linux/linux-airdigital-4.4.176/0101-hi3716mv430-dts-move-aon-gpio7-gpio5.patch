Subject: [PATCH] hi3716mv430 dts: move aon gpio7 --> gpio5

to keep compatibility with the 4.4.35 gpio numbers, which our driver
uses
---
 .../linux-4.4.y/arch/arm/boot/dts/hi3716mv430.dts    | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/source/kernel/linux-4.4.y/arch/arm/boot/dts/hi3716mv430.dts b/source/kernel/linux-4.4.y/arch/arm/boot/dts/hi3716mv430.dts
index 902c4c479..e94ac6048 100755
--- a/source/kernel/linux-4.4.y/arch/arm/boot/dts/hi3716mv430.dts
+++ b/source/kernel/linux-4.4.y/arch/arm/boot/dts/hi3716mv430.dts
@@ -167,8 +167,8 @@
 			gpio5: gpio5 {
 				compatible = "arm,pl061", "arm,primecell";
 				arm,primecell-periphid = <0x00041061>;
-				reg = <0xF8B25000 0x1000>;
-				interrupts = <0 58 4>;
+				reg = <0xF8004000 0x1000>;
+				interrupts = <0 26 0>;
 				gpio-controller;
 				#gpio-cells = <2>;
 				interrupt-controller;
@@ -180,8 +180,8 @@
 			gpio6: gpio6 {
 				compatible = "arm,pl061", "arm,primecell";
 				arm,primecell-periphid = <0x00041061>;
-				reg = <0xF8B26000 0x1000>;
-				interrupts = <0 59 4>;
+				reg = <0xF8B25000 0x1000>;
+				interrupts = <0 58 4>;
 				gpio-controller;
 				#gpio-cells = <2>;
 				interrupt-controller;
@@ -193,8 +193,8 @@
 			gpio7: gpio7 {
 				compatible = "arm,pl061", "arm,primecell";
 				arm,primecell-periphid = <0x00041061>;
-				reg = <0xF8004000 0x1000>;
-				interrupts = <0 26 0>;
+				reg = <0xF8B26000 0x1000>;
+				interrupts = <0 59 4>;
 				gpio-controller;
 				#gpio-cells = <2>;
 				interrupt-controller;
-- 
2.17.1

