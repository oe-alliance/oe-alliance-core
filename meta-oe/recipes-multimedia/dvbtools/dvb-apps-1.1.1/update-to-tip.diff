+struct vbi *open_null_vbi(struct cache *ca)
+{
+    static int inited = 0;
+    struct vbi *vbi;
+
+    if (not inited)
+    lang_init();
+    inited = 1;
+
+    vbi = malloc(sizeof(*vbi));
+    if (!vbi)
+    {
+		error("out of memory");
+		goto fail1;
+    }
+
+    vbi->fd = open("/dev/null", O_RDONLY);
+	if (vbi->fd == -1)
+	{
+		error("cannot open null device");
+		goto fail2;
+	}
+
+    vbi->ttpid = -1;
+    out_of_sync(vbi);
+    vbi->ppage = vbi->rpage;
+    fdset_add_fd(fds, vbi->fd, vbi_handler, vbi);
+    return vbi;
+
+fail3:
+    close(vbi->fd);
+fail2:
+    free(vbi);
+fail1:
+    return 0;
+}
