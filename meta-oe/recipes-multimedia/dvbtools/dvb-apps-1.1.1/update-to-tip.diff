+
+void vbi_del_handler(struct vbi *vbi, void *handler, void *data)
+{
+    struct vbi_client *cl;
+
+    for (cl = PTR vbi->clients->first; cl->node->next; cl = PTR cl->node->next)
+	if (cl->handler == handler && cl->data == data)
+	{
+	    dl_remove(cl->node);
+	    break;
+	}
+    return;
+}
