+    ev->type = type;
+    ev->i1 = i1;
+    ev->i2 = i2;
+    ev->i3 = i3;
+    ev->p1 = p1;
+    for (cl = PTR vbi->clients->first; cln = PTR cl->node->next; cl = cln)
+    cl->handler(cl->data, ev);
+}
