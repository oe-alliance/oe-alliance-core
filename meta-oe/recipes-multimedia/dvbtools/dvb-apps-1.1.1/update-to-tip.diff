+void xio_title(struct xio_win *xw, char *title)
+{
+    char buf[sizeof(xw->title) + 32];
+
+    if (strlen(title) >= sizeof(xw->title))
+	return; //TODO: trimm...
+    if (strcmp(xw->title, title) == 0)
