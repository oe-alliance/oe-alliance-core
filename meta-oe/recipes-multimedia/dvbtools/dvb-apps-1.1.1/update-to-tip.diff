+int hamm24(u8 *p, int *err)
+{
+    int e = hamm24par[0][p[0]] ^ hamm24par[1][p[1]] ^ hamm24par[2][p[2]];
+    int x = hamm24val[p[0]] + p[1] % 128 * 16 + p[2] % 128 * 2048;
+    *err += hamm24err[e];
+    return x ^ hamm24cor[e];
+}
+
+
+int chk_parity(u8 *p, int n)
+{
+    int err;
+    for (err = 0; n--; p++)
+	if (hamm24par[0][*p] & 32)
+	    *p &= 0x7f;
+	else
+	    *p = BAD_CHAR, err++;
+    return err;
+}
