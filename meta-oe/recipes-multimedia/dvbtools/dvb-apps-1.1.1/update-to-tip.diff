+void send_errmsg(struct vbi *vbi, char *errmsg, ...)
+{
+	va_list args;
+	if (errmsg == NULL || *errmsg == '\0')
+		return;
+	va_start(args, errmsg);
+	char *buff = NULL;
+	if (vasprintf(&buff, errmsg, args) < 0)
+		buff = NULL;
+	va_end(args);
+	if(buff == NULL)
+		out_of_mem(-1);
+	vbi_send(vbi, EV_ERR, 0, 0, 0, buff);
+}
